{
  "cSpell.words": [
    "cooldown",
    "gallerygrid",
    "hjschema",
    "huiji",
    "huijiwiki",
    "infobox",
    "knockback",
    "lootbox",
    "lootboxes",
    "navbox",
    "noinclude",
    "onlyinclude",
    "overfast",
    "overwatch",
    "owlib",
    "reflist",
    "Scribunto",
    "tabx",
    "ustring",
    "wikitable",
  ],
  "Lua.format.defaultConfig": {
    "quote_style": "double",
    "trailing_table_separator": "smart",
    "end_statement_with_semicolon": "same_line",
    "max_line_length": "1000",
  },
  "Lua.workspace.ignoreDir": [
    "output",
  ],
  // "[css]": {
  //   "editor.minimap.markSectionHeaderRegex": "^/\\* #region (?<label>.+) \\*/",
  // },
  "typescript.implementationsCodeLens.enabled": true,

  "overpy.workshopLanguage": "zh-CN",

  "wikiparser.articlePath": "https://overwatch.huijiwiki.com/wiki/$1",
  "wikiparser.color": false,
  "wikiparser.inlay": false,
  "wikiparser.linter.severity": "errors and warnings",
  "wikitext.wikiparser.enable": true,
  "Lua.runtime.version": "Lua 5.1",
}
